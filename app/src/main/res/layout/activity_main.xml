<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <LinearLayout
        android:layout_width="357dp"
        android:layout_height="391dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.555"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.56">

        <Button
            android:id="@+id/buttonFood"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="getFood"
            android:text="Получить еду" />

        <Button
            android:id="@+id/button4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="getTransports"
            android:text="Получить транспорт" />

        <Button
            android:id="@+id/buttonFields"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="getFields"
            android:text="Получить харак. полей" />

        <Button
            android:id="@+id/buttonEmployee"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="getContract"
            android:text="Получить харак. сотрудников" />

        <Button
            android:id="@+id/buttonPatch"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="patchData"
            android:text="Изменить данные" />

        <Button
            android:id="@+id/buttonDelete"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="deleteData"
            android:text="Удалить данные" />

        <Button
            android:id="@+id/button13"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="postFood"
            android:text="Вставить данные еды" />

        <Button
            android:id="@+id/button15"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:onClick="postFields"
            android:text="Вставить данные поля" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>